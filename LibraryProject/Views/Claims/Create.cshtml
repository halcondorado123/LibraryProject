<h1 class="bg-info text-white">Crear Notificación/Claim</h1>
<a asp-action="Index" class="btn btn-secondary">Atrás</a>
<div asp-validation-summary="All" class="text-danger"></div>

<form method="post">
    <div class="form-group">
        <label for="ClaimType">Tipo de Notificación:</label>
        <input name="ClaimType" class="form-control" />
    </div>
    <div class="form-group">
        <label for="ClaimValue">Valor de la Notificación:</label>
        <input name="ClaimValue" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Crear</button>
</form>
